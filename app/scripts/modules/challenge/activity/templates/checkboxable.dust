{>header /}

<form>
	<div class="checkboxable">
		{#data}
			<div class="checkboxable__optionset">
				<label>
					<input class="checkboxable__checkbox" name="checkboxable-optionset[{$idx}]" type="checkbox" value="{value}" />{text}

					{?subtext}
					<div class="checkboxable__subtext subtext">{subtext}</div>
					{/subtext}
				</label>
			</div>
		{/data}
	</div>
</form>

{>pagination /}