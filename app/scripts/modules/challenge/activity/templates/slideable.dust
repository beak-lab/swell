<div class="activity__title">{title}</div>
<div class="activity__description">
	{description|s}
</div>
<div class="activity__question">
	<h3>{question|s}</h3>
</div>

<div class="slideable">
	<div class="slideable__labels">
		<div class="slideable__label is-left-label">{leftLabel}</div>
		<div class="slideable__label is-right-label">{rightLabel}</div>
	</div>

	<input type="range" class="slideable__slider" data-snap-ignore='true'>
</div>

{>pagination /}