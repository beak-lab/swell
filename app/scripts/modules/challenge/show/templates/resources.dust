<div class="resources">	
{#resources}{#attributes}
    {@select key="{type}" type="string"}
    	{@eq value="quote"}
		<div class="resource resource__quote">
	        <p class="resource__quote__quote">{title}</p>
	        <p class="resource__quote__cite">{data}</p>
		</div>
    	{/eq}
    {/select}
{/attributes}{/resources}
</div>
