<?xml version='1.0' encoding='utf-8'?>
<widget id="nz.co.beak.swell" version="0.2.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Html5Video">
        <param name="android-package" value="org.apache.cordova.plugin.Html5Video" />
    </feature>
    <feature name="VideoPlayer">
        <param name="android-package" value="com.dawsonloudon.videoplayer.VideoPlayer" />
    </feature>
    <name>Swell</name>
    <description>
        Online well-being platform
    </description>
    <author email="rekarnar@gmail.com" href="http://beak.co.nz">
        Beak Labs
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
